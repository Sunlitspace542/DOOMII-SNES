FAILAT 21
ASSIGN EXISTS CRL: >NIL:
IF WARN
  RL:CRL/SetupCRL
ENDIF

DELETE RLDATA:WALLS/WALLS.DEF?? QUIET
DELETE RLDATA:IMAGES/IMAGES.DEF?? QUIET
DELETE RLDATA:WALLIMG/BANK??.DEF QUIET

COPY CRL:#?.A RL:
COPY CRL:IMG#? RL:
COPY CRL:WALL~(LIST)#? RL:

COPY CRL2:DATA/WALLS RL2:DATA/WALLS
COPY CRL2:DATA/IMAGES RL2:DATA/IMAGES
COPY CRL2:DATA/WALLIMG RL2:DATA/WALLIMG

TOUCH RL:rlwalls.a
TYPE RL:wallstart
TYPE RL:wallend

TOUCH RL:rlimages.a
TYPE RL:imgstart
TYPE RL:imgend

TF rlimages.i
